repeat
	wait()
until game:IsLoaded()

if game.PlaceId == 2788229376 then
	game.Players.LocalPlayer.Chatted:Connect(function(msg)
		if msg == "!name" then
			wait(0.3)
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(game.Players.LocalPlayer.Name, "All")
			setclipboard(game.Players.LocalPlayer.Name)
		end
	end)

	
